class Solution
{
    public:
        vector<int> sortedSquares(vector<int> &nums)
        {
            vector<int>ans;
            int n = nums.size();
            int num;
            for (int i = 0; i < n; i++)
            {
                num = nums[i] *nums[i];
                ans.push_back(num);
            }
            sort(ans.begin(), ans.end());
            return ans;
        }
   
};
